%% Initialising:

mode_setup;
thin#:=.4pt#; u#:=1pt#;
kk#:=2u#;
define_pixels(thin, u);


%% Encoding (accents by ISO-8859-2):

aacute = 225;  Aacute = 193;
bnarrow = 18;
ccaron = 232;  Ccaron = 200;
dcaron = 239;  Dcaron = 207;
eacute = 233;  ecaron = 236;  Eacute = 201;  Ecaron = 204;
iacute = 237;  Iacute = 205;
oacute = 243;  onarrow = 14;  oacutenarrow = 15; Oacute = 211;
ncaron = 242;  Ncaron = 210;
rcaron = 248;  Rcaron = 216;
scaron = 185;  Scaron = 169;
sleft = 10;  scaronleft = 11;  sdepth = 12;  scarondepth = 13;
tcaron = 187;  Tcaron = 171;
uacute = 250;  uring  = 249;  Uacute = 218;  Uring = 217;
vnarrow = 16;  wnarrow =17;
yacute = 253;  Yacute = 221;
zcaron = 190;  Zcaron = 174;

minus = 21;  dash = 22;  times = 23;
clqq = 254;  crqq = 255;
leftquota = 96;  rightquota = 39;
slash = 47;  backslash = 92;
percent = 37;  promile = 141;
inch = 34;

%% Macros and variables:

pair sklon[];
sklon1:=-(1.5,6); sklon2:=(5,6);
dotkoef=1.7;

def dotah =
  draw ((0,1){sklon1}..(1,0){right}..{sklon2}(8,6))
enddef;

def carka =
  draw ((0,9)--(1.5,13))
enddef;

def capcarka =
  draw ((0,15)--(1.5,18))
enddef;

def hacek =
  draw ((0,12)..{sklon1}(-.3,10)..{right}(.8,9)..tension2..{sklon2}(5,12))
enddef;

def krouzek =
   draw ((0,9){right}..(0,11){left}..cycle)
enddef;

def smycka =
   draw ((0,7){sklon1}..{sklon1}(-2,0)..{sklon1}(-3.1,-4)..
       {left}(-5.3,-7)..{-sklon1}(-5.5,-5)..
       (-2,0)..{sklon2}(5.5,6))
enddef;

def kcara =
   draw (9.5,14)--(5,0); draw (0,0)--(0,7)
enddef;

currenttransform:=currenttransform scaled u;
pickup pencircle scaled thin;
let clearpen=\;

%screenstrokes;

