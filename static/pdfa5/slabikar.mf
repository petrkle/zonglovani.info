%%%%%%%%%%%%%%%%%%%%%%% Digits %%%%%%%%%%%%%%%%%%%%%%%%%%%

beginchar("1", 8u#, 14u#, 0);
  z1=(1.5,9); z2=(5.5,14); z3=(2.5,0);
  draw z1..{dir60}z2--z3;
endchar;

beginchar("2", 8u#, 14u#, 0);
  z1=(1.5,11); z2=(4,14); z3=(6,11); z4=(0,0); z5=(5,0);
  draw z1..z2{right}..z3{-dir80}..{-dir65}z4--z5;
endchar;

beginchar("3", 8u#, 14u#, 0);
  z1=(1.5,11); z2=(4,14); z3=(6,11); z4=(3,8); z5=(2,0); z6=(-.5,3);
  draw z1..z2{right}..z3{-dir80}..z4&z4..tension1.5..z5{left}..z6{up};
endchar;

beginchar("4", 8u#, 14u#, 0);
  z1=(5,14); z2=(0,6); z3=(6,6); z4=(4.5,9); z5=(2.5,0);
  draw z1--z2--z3;
  draw z4--z5;
endchar;

beginchar("5", 8u#, 14u#, 0);
  z1=(6,14); z2=(2.5,14); z3=(1,8); z4=(4,9); z5=(2,0); z6=(-.5,3);
  draw z1--z2--z3&z3..z4..tension1.5..z5{left}..z6{up};
endchar;

beginchar("6", 8u#, 14u#, 0);
  z1=(6,14); z3=(.5,5); z4=(4,8); z5=(2,0); z6=(-.5,3);
  draw z1{-dir35}..z3{sklon1}&z3{-sklon1}..z4..tension1.5..z5{left}..
       z3{-sklon1};
endchar;

beginchar("7", 8u#, 14u#, 0);
  z1=(0.5,14); z2=(6,14); z3=(0,0); z4=(1.5,7); z5=(4.5,7);
  draw z1--z2--z3;
  draw z4--z5;
endchar;

beginchar("8", 8u#, 14u#, 0);
  z1=(4,14); z2=(3.2,8); z3=(2,0);
  draw z1{left}..z2..z3{left}..z2..{left}z1;
endchar;

beginchar("9", 8u#, 14u#, 0);
  z0=(2,13); z1=(6,14); z2=(1,7); z3=(2.5,0);
  z1p=(4,13.9);
  draw z1{left}..z1p..z0{-sklon2}..tension1.5..
       z2{right}..tension1.2..{-sklon1}z1{sklon1}..z3{sklon1};
endchar;

beginchar("0", 8u#, 14u#, 0);
  z1=(4,14); z3=(1,0);
  draw z1{left}..tension2.9..z3{right}..tension2.9..{left}z1;
endchar;


%%%%%%%%%%%%%%%%%%%%% Others %%%%%%%%%%%%%%%%%%%%%%%%%%%%%

beginchar("+", 8u#, 9u#, 0);
  z1=(1,7); z2=(6,7); z3=(3.3,4.5); z4=(3.7,9.5);
  draw z1--z2;
  draw z3--z4;
endchar;

beginchar(minus, 8u#, 7u#, 0);
  z1=(1,7); z2=(6,7);
  draw z1--z2;
endchar;

beginchar(times, 8u#, 9u#, 0);
  z1=(1.7,9); z2=(5.7,9); z3=(1.3,5); z4=(5.3,5);
  draw z1--z4;
  draw z2--z3;
endchar;

beginchar("*", 8u#, 9u#, 0);
  z1=(1.7,10); z2=(5.7,10); z3=(1.3,4); z4=(5.3,4);
  draw z1--z4;
  draw z2--z3;
  draw (0,7)--(7,7);
endchar;

beginchar("=", 8u#, 9u#, 0);
  z1=(1,6); z2=(6,6); z3=(1.2,8); z4=(6.2,8);
  draw z1--z2;
  draw z3--z4;
endchar;

beginchar(slash, 8u#, 14u#, 0);
  z1=(0,0); z2=(9,14);
  draw z1--z2;
endchar;

beginchar(backslash, 6u#, 14u#, 0);
  z1=(0,14); z2=(6,0);
  draw z1--z2;
endchar;

beginchar("<", 8u#, 14u#, 0);
  z1=(1,7); z2=(7,10); z3=(6,4);
  draw z3--z1--z2;
endchar;

beginchar(">", 8u#, 14u#, 0);
  z1=(7,7); z2=(2,10); z3=(1,4);
  draw z3--z1--z2;
endchar;

beginchar("@", 9u#, 9u#, 2u#);
  z0=(4,6); z1=(7,7); z2=(3,0); z3=(5.4,1);
  z1p=(5.5,6.9);
  z4=(6,0); z5=(9,4);  z6=(7,9);  z7=(1,0);  z8=(7,-1);
  draw z1{left}..z1p..z0{-sklon2}..tension1.5..
       z2{right}..tension1.2..{-sklon1}z1{sklon1}..z3{sklon1}..
       z4{right}..z5..z6..z7{dir-60}..z8;
endchar;

beginchar("(", 8u#, 16u#, 2u#);
  z1=(7,16); z2=(2,7); z3=(5,-2);
  draw z1..z2..z3;
endchar;

beginchar(")", 8u#, 16u#, 2u#);
  z1=(3,16); z2=(6,7); z3=(1,-2);
  draw z1..z2..z3;
endchar;

beginchar("[", 8u#, 14u#, 2u#);
  z1=(7,14); z1a=(4,14); z2a=(2,0); z2=(5,0);
  draw z1--z1a--z2a--z2;
endchar;

beginchar("]", 8u#, 14u#, 2u#);
  z1=(6,14); z1a=(3,14); z2a=(1,0); z2=(4,0);
  draw z1a--z1--z2--z2a;
endchar;

beginchar("{", 8u#, 14u#, 0);
  z1=(7,14); z7=(5.5,0);  z1a=(5,14);  z2a=(3,0);
  z2-z1a = whatever*(z2a-z1a);  y2=12;
  z3-z1a = whatever*(z2a-z1a);  y3=9;
  z4 = (2,7);
  z5-z1a = whatever*(z2a-z1a);  y5=5;
  z6-z1a = whatever*(z2a-z1a);  y6=2;
  draw z1{left}..z2..z3..z4&z4..z5..z6..{right}z7;
endchar;

beginchar("}", 8u#, 14u#, 0);
  z1=(2.5,14); z7=(1,0);  z1a=(5,14);  z2a=(3,0);
  z2-z1a = whatever*(z2a-z1a);  y2=12;
  z3-z1a = whatever*(z2a-z1a);  y3=9;
  z4 = (7,7);
  z5-z1a = whatever*(z2a-z1a);  y5=5;
  z6-z1a = whatever*(z2a-z1a);  y6=2;
  draw z1{right}..z2..z3..z4&z4..z5..z6..{left}z7;
endchar;

beginchar(percent, 10u#, 14u#, 0);
  z1=(0,0); z2=(9,14);  z3=(3,14);  z4=(2,9);
  draw z1--z2;
  draw z3..tension2..z4..tension2..cycle;
  draw (z3..tension2..z4..tension2..cycle) shifted (4,-y4);
endchar;

beginchar(promile, 13u#, 14u#, 0);
  z1=(0,0); z2=(9,14);  z3=(3,14);  z4=(2,9);
  draw z1--z2;
  draw z3..tension2..z4..tension2..cycle;
  draw (z3..tension2..z4..tension2..cycle) shifted (4,-y4);
  draw (z3..tension2..z4..tension2..cycle) shifted (8,-y4);
endchar;

beginchar("&", 7u#, 11u#, 0);
  z1=(5,0); z2=(3,7); z3=(4,11); z4=(0,1.5);  z5=(1,0);  z6=(7,7);
  draw z1..z2..z3{right}..z2..tension2..z4{sklon1}..z5{right}..tension2..
  {sklon2}z6;
endchar;

beginchar("$", 8u#, 15u#, 1u#);
  z1=(8,10.5); z2=(4.5,14); z2a=(1,10.5);
  z3=(4,7);
  z4=(3.5,0); z4a=(7,3.5);  z5=(0,3.5);
  draw z1..z2{left}..z2a..z3..z4a..z4{left}..z5;
  draw (2.5,-1)--(3.5,15);
  draw (4.5,-1)--(5.5,15);
endchar;

beginchar("#", 11.5u#, 14u#, 0);
  z1=(0,0); z2=(4,14);  z3=(10.5,0);
  draw (z1--z2) shifted (2,0);
  draw (z1--z2) shifted (6,0);
  draw (z1--z3) shifted (.5,5);
  draw (z1--z3) shifted (1.3,9);
endchar;

beginchar("|", 2u#, 14u#, 0);
  z1=(0,0); z2=(2,14);
  draw (z1--z2)
endchar;

beginchar("~", 8u#, 9u#, 0);
  z1=(0,6); z2=(3,8); z3=(5,6); z4=(8,8);
  draw z1..z2..z3..z4;
endchar;

beginchar(inch, 4u#, 15u#, 0);
  draw (1.8,15)--(1.5,12);
  draw (3.8,15)--(3.5,12);
endchar;

beginchar("^", 6u#, 14u#, 0);
  draw (2,12)--(4,14)--(6,12);
endchar;

beginchar("_", 8u#, 0, 7u#);
  draw (0,-7)--(8,-7);
endchar;


%%%%%%%%%%%%%%%%%%%%% Punctuation %%%%%%%%%%%%%%%%%%%%%%%%

beginchar("-", 3u#, 7u#, 0);
  draw (0,4)--(3,4);
endchar;

beginchar(dash, 12u#, 7u#, 0);
  z1=(1,7); z2=(10,7);
  draw z1--z2;
endchar;

beginchar("!", 6u#, 14u#, 0);
  pickup pencircle scaled (dotkoef*thin);
  drawdot (2,0);
  pickup pencircle scaled thin;
  draw (2.6, 4)..(5,14);
endchar;

beginchar("?", 8u#, 14u#, 0);
  pickup pencircle scaled (dotkoef*thin);
  drawdot (4,0);
  pickup pencircle scaled thin;
  z1=(3,12);  z2=(6,14);  z3=(5,8.5);  z4=(4.5,3);  z5=(7,5);
  draw z1..z2{right}..z3..z4{right}..z5;
endchar;

beginchar(",", 3u#, 1u#, 0);
  draw (0,1)--(-1,-2);
endchar;

beginchar(".", 3u#, 1u#, 0);
  pickup pencircle scaled (dotkoef*thin);
  drawdot (0,0);
  pickup pencircle scaled thin;
endchar;

beginchar(":", 4u#, 7u#, 0);
  pickup pencircle scaled (dotkoef*thin);
  drawdot (1,0);  drawdot (2,7);
  pickup pencircle scaled thin;
endchar;

beginchar(";", 4u#, 7u#, 0);
  pickup pencircle scaled (dotkoef*thin);
  drawdot (2,7);
  pickup pencircle scaled thin;
  draw (1,1)--(0,-2);
endchar;

beginchar(clqq, 4u#, 1u#, 0);
  draw (0,1)--(-1,-2);
  draw (2,1)--(1,-2);
endchar;

beginchar(crqq, 4u#, 15u#, 0);
  draw (2,15)--(1.5,12);
  draw (4,15)--(3.5,12);
endchar;

beginchar(leftquota, 4u#, 15u#, 0);
  draw (0,15){down}..(1,12);
endchar;

beginchar(rightquota, 4u#, 15u#, 0);
  draw (4,15){down}..(2,12);
endchar;



%%%%%%%%%%%%%%%%%%%%% special parts of characters %%%%%%%%%%%%%%%%%

beginchar(1, .7u#, 7u#, 0);  %% pravy konec znaku
  draw (0,6)..(.7,7);
endchar;

beginchar(2, 5u#, 7u#, 0);  %% levy rovny zacatek znaku
  draw (0,0)..(5,6);
endchar;

beginchar(3, 6u#, 7u#, 0);  %% levy prohnuty zacatek znaku
  draw (0,0){(3,2)}..{sklon2}(6,6);
endchar;

beginchar(4, 5u#, 7u#, 0);  %% obecna konvexni spojka za verzalkou a s
  draw (-4,0){right}..{sklon2}(5,6);
endchar;

beginchar(5, 7u#, 7u#, 0);  %% delsi konvexni spojka za verzalkou P
  draw (-4,0){right}..{sklon2}(7,6);
endchar;

beginchar(6, 3u#, 7u#, 0);  %% kratsi konvexni spojka pro dvojice sv sn
  draw (-4,0){right}..{sklon2}(3,6);
endchar;

beginchar(7, 8u#, 7u#, 0);  %% nabehova carka pro male x
  draw (0,2){down}..(2,0){right}..tension2..{sklon2}(8,6);
endchar;


%%%%%%%%%%%%%%%%%%%%%% Ligtables %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

boundarychar:=1;
leftboundary:=1;

ligtable ",":  "," =: clqq;
ligtable leftquota:  leftquota =: crqq, skipto leftboundary;
ligtable "-":  "-" =: minus, skipto leftboundary;
ligtable minus: "-" =: dash, skipto leftboundary;

def rightboundaries =
      1 |=:> 1,
      "-" |=:|>> 1,
      "!" |=:|>> 1,
      "?" |=:|>> 1,
      "," |=:|>> 1,
      ":" |=:|>> 1,
      ";" |=:|>> 1,
      crqq |=:|>> 1,
      rightquota |=:|>> 1,
      slash |=:|>> 1,
      "&" |=:|>> 1,
      "+" |=:|>> 1,
      "]" |=:|>> 1,
      ")" |=:|>> 1,
      "." |=:|>> 1
enddef;

ligtable  ||: clqq: dash: slash: "(": "[": "&": "+": leftboundary::
      "a" kern kk#,  aacute kern kk#,
      "b" |=:|> 3,
      "c" kern kk#, ccaron kern kk#,
      "d" kern kk#, dcaron kern kk#,
      "e" |=:|> 3, eacute |=:|> 3, ecaron |=:|> 3,
      "f" |=:|> 3,
      "g" kern kk#,
      "h" |=:|> 3,
      "i" |=:|> 3, iacute |=:|> 3,
      "j" |=:|> 3,
      "k" |=:|> 3,
      "l" |=:|> 3,
      "m" |=:|> 2,
      "n" |=:|> 2,  ncaron |=:|> 2,
      "o" kern kk#,  oacute kern kk#,
      "p" |=:|> 3,  "q" kern kk#,
      "r" |=:|> 3,  rcaron |=:|> 3,
      "s" |=:> sleft,  scaron |=:> scaronleft,
      "u" |=:|> 3,  uring |=:|> 3, uacute |=:|> 3,
      "t" |=:|> 3,  tcaron |=:|> 3,
      "v" |=:|> 2,  "w" |=:|> 2,
      "x" |=:|> 7,
      "y" |=:|> 2,  yacute |=:|> 2,
      "z" |=:|> 2,  zcaron |=:|> 2;

ligtable "B": "D": Dcaron: "F": "I": Iacute: "O": Oacute:
   "S": Scaron:
   "T": Tcaron: "V": "W":
      "a" |=:| 4,  aacute |=:| 4,
      "b" |=:| 4,
      "c" |=:| 4,  ccaron |=:| 4,
      "d" |=:| 4,  dcaron |=:| 4,
      "e" |=:| 4, eacute |=:| 4, ecaron |=:| 4,
      "f" |=:| 4,
      "g" |=:| 4,
      "h" |=:| 4,
      "i" |=:| 4, iacute |=:| 4,
      "j" |=:| 4,
      "k" |=:| 4,
      "l" |=:| 4,
      "m" |=:| 4,
      "n" |=:| 4,  ncaron |=:| 4,
      "o" |=:| 4,  oacute |=:| 4,
      "p" |=:| 4,  "q" |=:| 4,
      "r" |=:| 4,  rcaron |=:| 4,
      "s" |=:| 4,  scaron |=:| 4,
      "u" |=:| 4,  uring |=:| 4, uacute |=:| 4,
      "t" |=:| 4,  tcaron |=:| 4,
      "v" |=:| 4,  "w" |=:|> 4,
      "x" |=:| 4,
      "y" |=:| 4,  yacute |=:| 4,
      "z" |=:| 4,  zcaron |=:| 4;

ligtable "P":
      "a" |=:| 5,  aacute |=:| 5,
      "b" |=:| 5,
      "c" |=:| 5,  ccaron |=:| 5,
      "d" |=:| 5,  dcaron |=:| 5,
      "e" |=:| 5, eacute |=:| 5, ecaron |=:| 5,
      "f" |=:| 5,
      "g" |=:| 5,
      "h" |=:| 5,
      "i" |=:| 5, iacute |=:| 5,
      "j" |=:| 5,
      "k" |=:| 5,
      "l" |=:| 5,
      "m" |=:| 5,
      "n" |=:| 5,  ncaron |=:| 5,
      "o" |=:| 5,  oacute |=:| 5,
      "p" |=:| 5,  "q" |=:| 5,
      "r" |=:| 5,  rcaron |=:| 5,
      "s" |=:| 5,  scaron |=:| 5,
      "u" |=:| 5,  uring |=:| 5, uacute |=:| 5,
      "t" |=:| 5,  tcaron |=:| 5,
      "v" |=:| 5,  "w" |=:|> 5,
      "x" |=:| 5,
      "y" |=:| 5,  yacute |=:| 5,
      "z" |=:| 5,  zcaron |=:| 5;

ligtable
   scarondepth:  scaronleft:  scaron:
      "!" kern 1u#, "?" kern 2u#,
   "s":  sleft:  sdepth:
      "a" |=:| 4,  aacute |=:| 4,
      "b" |=:| 4,
      "c" |=:| 4,  ccaron |=:| 4,
      "d" |=:| 4,  dcaron |=:| 4,
      "e" |=:| 4, eacute |=:| 4, ecaron |=:| 4,
      "f" |=:| 4,
      "g" |=:| 4,
      "h" |=:| 4,
      "i" |=:| 4, iacute |=:| 4,
      "j" |=:| 4,
      "k" |=:| 4,
      "l" |=:| 4,
      "m" |=:| 6,
      "n" |=:| 6,  ncaron |=:| 6,
      "o" |=:| 4,  oacute |=:| 4,
      "p" |=:| 4,  "q" |=:| 4,
      "r" |=:| 4,  rcaron |=:| 4,
      "s" |=:| 4,  scaron |=:| 4,
      "u" |=:| 4,  uring |=:| 4, uacute |=:| 4,
      "t" |=:| 6,  tcaron |=:| 6,
      "v" |=:| 6,  "w" |=:|> 6,
      "x" |=:| 4,
      "y" |=:| 6,  yacute |=:| 6,
      "z" |=:| 6,  zcaron |=:| 6;


ligtable "b":
     "m" =:| bnarrow,
     "n" =:| bnarrow,  ncaron =:| bnarrow,
     "v" =:| bnarrow,  "w" =:| bnarrow,
     "y" =:| bnarrow,  yacute =:| bnarrow,
     rightboundaries;
ligtable "o":
     "m" =:| onarrow,
     "n" =:| onarrow,  ncaron =:| onarrow,
     "v" =:| onarrow,  "w" =:| onarrow,
     "y" =:| onarrow,  yacute =:| onarrow,
     rightboundaries;
ligtable oacute:
     "m" =:| oacutenarrow,
     "n" =:| oacutenarrow,  ncaron =:| oacutenarrow,
     "v" =:| oacutenarrow,  "w" =:| oacutenarrow,
     "y" =:| oacutenarrow,  yacute =:| oacutenarrow,
     rightboundaries;
ligtable "v":
     "m" =:| vnarrow,
     "n" =:| vnarrow,  ncaron =:| vnarrow,
     "v" =:| vnarrow,  "w" =:| vnarrow,
     "y" =:| vnarrow,  yacute =:| vnarrow,
     rightboundaries;
ligtable "w":
     "m" =:| wnarrow,
     "n" =:| wnarrow,  ncaron =:| wnarrow,
     "v" =:| wnarrow,  "w" =:| wnarrow,
     "y" =:| wnarrow,  yacute =:| wnarrow,
     rightboundaries;

ligtable
   "g": "G":
   "j": "J":
   "q": "Q":
   "y": yacute: "Y": Yacute:
        "s" |=: sdepth,  scaron |=: scarondepth,
        rightboundaries;

ligtable "a": aacute: "A": Aacute:
   "c": ccaron: "C": Ccaron:
   "d": dcaron:
   "e": eacute: ecaron: "E": Eacute: Ecaron:
   "f":
   "h": "H":
   "i": iacute:
   "k": "K":
   "l": "L":
   "m": "M":
   "n": ncaron: "N": Ncaron:
   "p":
   "r": rcaron: "R": Rcaron:
   "t": tcaron:
   "u": uring: uacute: "U": Uacute: Uring:
   "x": "X":
   "z": zcaron: "Z": Zcaron:
        rightboundaries;

fontdimen 1: .3pt#, 3u#, 2.5u#, 1u#, 7u#, 20u#, 2.5u#;
designsize := 25u#;

end;

