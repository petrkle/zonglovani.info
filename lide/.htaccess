RewriteEngine	on
RewriteBase /lide

RewriteCond %{HTTP_HOST} ^www\.(.*)
RewriteRule (.*) http://%1/lide/$1 [R=301,QSA,NE,L]

RewriteRule ^o/(.+)/(.+)/(.+).html$ overeni-emailu.php?m=$2@$1&k=$3 [L]
RewriteRule ^z/(.+)/(.+)/(.+).html$ obnova-hesla.php?m=$2@$1&k=$3 [L]
RewriteRule ^e/(.+)/(.+)/(.+).html$ zruseni.php?m=$2@$1&k=$3 [L]
RewriteRule ^p/(.+)/(.+)/(.+).html$ zmena-emailu.php?m=$2@$1&k=$3 [L]
RewriteRule ^sendmail/(.+).html$ sendmail.php?m=$1 [L]

RewriteRule ^dovednost$ /lide/dovednost/ [R=301,L]
RewriteRule ^dovednost/$ dovednost.php [L]
RewriteRule ^dovednost/(.+)\.html$ dovednost.php?filtr=$1 [L]

RewriteRule ^misto$ /lide/misto/ [R=301,L]
RewriteRule ^misto/$ misto.php [L]
RewriteRule ^misto/(.+)\.html$ misto.php?filtr=$1 [L]

RewriteRule ^stranka([^/]+)$ /lide/stranka$1/ [R=301,L]
RewriteRule ^stranka(.+)/$ index.php?pageID=$1 [L]
RewriteRule ^(.+)\.html$ uzivatel.php?id=$1 [L]
RewriteRule ^foto/(.+)\.jpg$ foto.php?id=$1 [L]
RewriteRule ^uzivatele.rss$ rss.php [L]

RewriteRule ^nastaveni$ /lide/nastaveni/ [R=301,L]
RewriteRule ^nastaveni/$ nastaveni.php [L]
RewriteRule ^nastaveni/(.+)$ nastaveni.php?uprav=$1 [L]
