RewriteEngine	on
RewriteBase /video

RewriteCond %{HTTP_HOST} ^www\.(.*)
RewriteRule (.*) http://%1/video/$1 [R=301,QSA,NE,L]

RewriteRule ^navod/info\.html$ navody-info.php [L]
RewriteRule ^stranka1\.html$ /video/ [R=301,L]
RewriteRule ^([^\/]+)/stranka1\.html$ /video/$1/ [R=301,L]
RewriteRule ^([^\/]+)/stranka(.*)\.html$ event.php?id=$1&pageID=$2 [L]
RewriteRule ^stranka(.*)\.html$ index.php?pageID=$1 [L]
RewriteRule ^([^\/]+)/(.*)\.html$ video.php?event=$1&v=$2 [L]
RewriteRule ^(.*)\.html video.php?v=$1 [L]
RewriteRule ^([^\/]+)/$ event.php?id=$1 [L]

RewriteCond %{REQUEST_FILENAME}	^.*\.(inc|sh)$
RewriteRule ^(.*)$ $1 [F]
